# StudyFlow Backend Environment Variables

# Database (PostgreSQL via Docker)
DATABASE_URL=postgresql://studyflow:studyflow_password@postgres:5432/studyflow

# GLM-4.7 API (Zhipu AI)
GLM_API_KEY=your-glm-api-key-here

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000

# Environment
ENVIRONMENT=production

# Logging
LOG_LEVEL=info

# Server Port
PORT=8000
